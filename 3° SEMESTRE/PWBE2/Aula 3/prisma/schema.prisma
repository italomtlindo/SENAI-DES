generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model carro {
  id         Int      @id @default(autoincrement())
  placa      String
  marca      String
  modelo     Int
  ano        Int
 
}

model clientes {
  id        Int     @id @default(autoincrement())
  nome      String
  cpf       Int
  email     String
  cnh       String
 
}